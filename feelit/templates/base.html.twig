<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('build/global.css') }}" />
        <link rel="stylesheet" href="{{ asset('build/app.css') }}">.
    {% endblock %}
</head>
<body>
<div class="container">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="{{ path('home') }}">Home</a></li>
            <li class="breadcrumb-item"><a href="{{ path('company_index') }}">Company</a></li>
            <li class="breadcrumb-item"><a href="{{ path('employee_index') }}">Employee</a></li>
            <li class="breadcrumb-item"><a href="{{ path('role_index') }}">Role</a></li>
            <li class="breadcrumb-item"><a href="{{ path('app_logout') }}">Log out</a></li>
        </ol>
    </nav>
    {% block body %}{% endblock %}
</div>
{% block javascripts %}
    <script src="{{ asset('build/app.js') }}"></script>
{% endblock %}
</body>
</html>